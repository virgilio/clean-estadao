{
    "manifest_version": 2,
    
    "name": "Estadão sem Bloqueio",
    "description": "Essa extensão remove a tela que obriga login no site do Estadão.",
    "version": "0.5.7",
    "icons": { "128": "icon.png" },    
    "content_scripts": [
        {
            "matches": ["http://*.estadao.com.br/*"],
            "js": ["jquery.js", "estadao.js"],
            "run_at": "document_end"
        }
    ]
}
